{
  "indentWidth": 2,
  "incremental": true,
  "typescript": {
    "quoteStyle": "preferSingle"
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "dockerfile": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,dockerfile}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    ".next/**",
    "/public/*",
    ".idea/*",
    ".vscode/*"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.64.2.wasm",
    "https://plugins.dprint.dev/json-0.14.1.wasm",
    "https://plugins.dprint.dev/markdown-0.12.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.2.wasm"
  ]
}
