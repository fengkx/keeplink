{
  "name": "keeplink",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@6.32.0",
  "scripts": {
    "lint": "xo",
    "test": "xo",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@metascraper/helpers": "^5.25.8",
    "@next/bundle-analyzer": "^12.1.0",
    "@prisma/client": "^3.10.0",
    "@supabase/supabase-js": "^1.30.7",
    "@supabase/ui": "^0.36.4",
    "@tailwindcss/forms": "^0.5.0",
    "@yaireo/tagify": "^4.9.8",
    "cookie": "^0.4.2",
    "date-fns": "^2.28.0",
    "execa": "^6.1.0",
    "he": "^1.2.0",
    "metascraper": "^5.25.8",
    "metascraper-description": "^5.25.8",
    "metascraper-title": "^5.25.8",
    "metascraper-url": "^5.25.8",
    "next": "^12.1.0",
    "next-pwa": "^5.4.5",
    "placeholder-loading": "^0.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "^2.0.9",
    "react-hook-form": "^7.27.1",
    "react-toast-notifications": "^2.5.1",
    "react-use": "^17.3.2",
    "single-file": "github:gildas-lormeau/SingleFile",
    "swr": "^1.2.2",
    "user-agents": "^1.0.945",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/he": "^1.1.2",
    "@types/react": "^17.0.39",
    "@types/user-agents": "^1.0.2",
    "autoprefixer": "^10.4.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prisma": "^3.10.0",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2",
    "xo": "^0.48.0"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "rules": {
      "import/extensions": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/filename-case": "off",
      "import/no-anonymous-default-export": "off",
      "@typescript-eslint/restrict-template-expressions": "off",
      "@typescript-eslint/no-implicit-any-catch": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "no-restricted-imports": "off",
      "unicorn/prefer-module": "off",
      "@typescript-eslint/no-unsafe-assignment": "off",
      "@typescript-eslint/no-unsafe-call": "off"
    },
    "envs": [
      "es2020",
      "node"
    ],
    "ignores": [
      "public/workbox-*.js",
      "public/sw.js"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,md,json,mjs}": "xo --fix",
    "*.prisma": "prisma format"
  }
}
