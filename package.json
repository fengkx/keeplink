{
  "name": "keeplink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "xo",
    "test": "xo",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@metascraper/helpers": "^5.25.8",
    "@next/bundle-analyzer": "^10.2.3",
    "@prisma/client": "^2.30.3",
    "@supabase/supabase-js": "^1.30.6",
    "@supabase/ui": "^0.24.3",
    "@tailwindcss/forms": "^0.3.4",
    "@yaireo/tagify": "^4.9.8",
    "cookie": "^0.4.2",
    "date-fns": "^2.28.0",
    "execa": "^5.1.1",
    "he": "^1.2.0",
    "metascraper": "^5.25.8",
    "metascraper-description": "^5.25.8",
    "metascraper-title": "^5.25.8",
    "metascraper-url": "^5.25.8",
    "next": "^10.2.3",
    "next-pwa": "^5.4.4",
    "placeholder-loading": "^0.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "^2.0.9",
    "react-hook-form": "^7.27.1",
    "react-toast-notifications": "^2.5.1",
    "react-use": "^17.3.2",
    "single-file": "github:gildas-lormeau/SingleFile",
    "swr": "^0.5.7",
    "user-agents": "^1.0.935",
    "zod": "^1.11.17"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/he": "^1.1.2",
    "@types/react": "^17.0.39",
    "@types/user-agents": "^1.0.2",
    "autoprefixer": "^10.4.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prisma": "^2.30.3",
    "tailwindcss": "^2.2.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.5",
    "xo": "^0.39.1"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "rules": {
      "unicorn/no-array-reduce": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/filename-case": "off",
      "import/no-anonymous-default-export": "off",
      "@typescript-eslint/restrict-template-expressions": "off",
      "@typescript-eslint/no-implicit-any-catch": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "no-restricted-imports": "off"
    },
    "envs": [
      "es2020",
      "node"
    ],
    "ignores": [
      "public/workbox-*.js",
      "public/sw.js"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,md,json,mjs}": "xo --fix",
    "*.prisma": "prisma format"
  }
}
